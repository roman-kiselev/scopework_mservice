{
    "name": "server",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "cross-env NODE_ENV=developmentJob nest start --watch",
        "startJob:dev": "cross-env NODE_ENV=developmentJob nest start --watch",
        "startHome": "cross-env NODE_ENV=productionHome nest start",
        "startHome:dev": "cross-env NODE_ENV=developmentHome nest start --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "prepare": "husky",
        "fix": "eslint src --ext .ts,.tsx --fix",
        "precommit": "concurrently \"npm run lint\" \"npm run fix\" \"npm run format\""
    },
    "dependencies": {
        "@nestjs/cache-manager": "^2.2.2",
        "@nestjs/common": "^10.0.0",
        "@nestjs/config": "^3.0.0",
        "@nestjs/core": "^10.0.0",
        "@nestjs/jwt": "^10.1.0",
        "@nestjs/microservices": "^10.3.3",
        "@nestjs/platform-express": "^10.0.0",
        "@nestjs/platform-socket.io": "^10.2.8",
        "@nestjs/sequelize": "^10.0.0",
        "@nestjs/swagger": "^7.1.1",
        "@nestjs/throttler": "^6.2.1",
        "@nestjs/websockets": "^10.2.8",
        "amqp-connection-manager": "^4.1.14",
        "amqplib": "^0.10.3",
        "bcrypt": "^5.1.0",
        "cache-manager": "^5.5.2",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "concurrently": "^8.2.2",
        "cross-env": "^7.0.3",
        "exceljs": "^4.4.0",
        "husky": "^9.1.4",
        "ioredis": "^5.4.1",
        "mysql2": "^3.5.1",
        "nestjs-throttler-storage-redis": "^0.5.1",
        "pg": "^8.13.1",
        "reflect-metadata": "^0.2.2",
        "rxjs": "^7.8.1",
        "sequelize": "^6.32.1",
        "sequelize-typescript": "^2.1.5",
        "socket.io": "^4.7.2"
    },
    "devDependencies": {
        "@nestjs/cli": "^10.0.0",
        "@nestjs/schematics": "^10.0.0",
        "@nestjs/testing": "^10.3.3",
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.2",
        "@types/node": "^20.3.1",
        "@types/sequelize": "^4.28.15",
        "@types/supertest": "^2.0.12",
        "@typescript-eslint/eslint-plugin": "^5.59.11",
        "@typescript-eslint/parser": "^5.59.11",
        "eslint": "^8.42.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-prettier": "^4.2.1",
        "jest": "^29.5.0",
        "prettier": "^2.8.8",
        "redis": "^4.6.14",
        "source-map-support": "^0.5.21",
        "supertest": "^6.3.3",
        "ts-jest": "^29.1.0",
        "ts-loader": "^9.4.3",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.1.3"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "moduleDirectories": [
            "<rootDir>/../",
            "node_modules"
        ],
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
